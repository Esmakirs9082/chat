# Production Environment Variables
# Этот файл содержит production defaults

# ====================================
# Production API
# ====================================
VITE_API_URL=https://api.ai-chat.com/api
VITE_WS_URL=wss://ws.ai-chat.com/ws

# ====================================
# Production Features
# ====================================
VITE_DEBUG_MODE=false
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_SENTRY=true
VITE_USE_MOCK_API=false
VITE_SHOW_PERFORMANCE=false
VITE_ENABLE_SW=true
VITE_ENABLE_PWA=true
VITE_HMR=false

# ====================================
# Production Third-party
# ====================================
VITE_SENTRY_ENVIRONMENT=production
VITE_APP_ENV=production
VITE_ENABLE_ANALYTICS=true

# ====================================
# Production Security
# ====================================
VITE_SECURE_COOKIES=true
VITE_TOKEN_EXPIRY=3600
VITE_CONNECTION_TIMEOUT=30000
VITE_RATE_LIMIT_RPM=30
VITE_MESSAGE_RATE_LIMIT=5

# ====================================
# Production Content
# ====================================
VITE_CONTENT_FILTER_LEVEL=strict
VITE_ENABLE_MODERATION=true